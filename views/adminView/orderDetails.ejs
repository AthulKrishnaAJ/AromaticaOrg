        <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
        <!-- <script src="admin-assets/js/vendors/jquery-3.6.0.min.js"></script> -->


        <script>

            $(document).ready(function () {
                $('#saveBtn').click(function () {
                    let newStatus = $('#statusSelect').val()
                    let orderId = '<%=order._id%>'

                    let checknewStatus;

                    if (newStatus === "pending" || newStatus === "processing" || newStatus === "shipped" || newStatus === "delivered" || newStatus === "cancelled" || newStatus === "returned") {
                        checknewStatus = newStatus
                    }
                    else {
                        Toastify({
                            text: "Please select a status",
                            duration: 3000,
                            close: true,
                            backgroundColor: "#dc3545",
                        }).showToast();
                        return false;
                    }

                    if (checknewStatus) {
                        $.ajax({
                            url: '/admin/changeStatus',
                            method: 'post',
                            data: {
                                orderId: orderId,
                                checknewStatus: checknewStatus
                            },
                            success: (response) => {
                                if (response.status === true) {
                                    Toastify({
                                        text: response.message,
                                        duration: 3000,
                                        close: true,
                                        backgroundColor: "#28a745",
                                    }).showToast();

                                    setTimeout(() => {
                                        location.href = "/admin/orderList"
                                    }, 3000);
                                }
                                else {
                                    Toastify({
                                        text: response.message,
                                        duration: 3000,
                                        close: true,
                                        backgroundColor: "#dc3545",
                                    }).showToast();
                                }
                            },
                            error: (error) => {
                                console.log("ajaxxx error", error.message)
                            }
                        })
                    }
                })
            })

        </script>


