            <div class="card">
                <header class="card-header">
                    <div class="row align-items-center">
                        <div class="col-lg-6 col-md-6 mb-lg-0 mb-15">
                            <span>
                                <i class="material-icons md-calendar_today"></i> <b>
                                    <%=order.createdAt%>
                                </b>
                            </span> <br>
                            <small class="text-muted">Order ID : <%=order._id%> </small>
                        </div>
                        <div class="col-lg-6 col-md-6 ms-auto text-md-end">
                            <% if(order.status !=="cancelled" && order.status !=="delivered" && order.status
                                !=="returned" && order.status !=="payment failed" ){%>
                                <select id="statusSelect" class="form-select d-inline-block mb-lg-0 mb-15 mw-200">
                                    <option>Change status</option>
                                    <% if(order.status !=="return pending" ){%>
                                        <option>pending</option>
                                        <option>processing</option>
                                        <option>shipped</option>
                                        <option>delivered</option>
                                        <%}else{%>
                                            <option>returned</option>
                                            <%}%>
                                </select>
                                <a class="btn btn-primary" id="saveBtn" type="button">Save</a>
                                <a class="btn btn-secondary print ms-2" href="#"><i
                                        class="icon material-icons md-print"></i></a>
                                <%}else{%>
                                    <% if(order.status==="cancelled" ){%>
                                        <h6>status: <span class="text-danger">Order <%=order.status%></span></h6>
                                        <h6>Reason for cancellation : <span class="text-muted" style="font-size: 12px;">
                                                <%=order.cancellationReason%>
                                            </span></h6>
                                        <%}%>

                                            <% if(order.status==="delivered" ){%>
                                                <h6>status: <span class="text-success">Order <%=order.status%></span>
                                                </h6>
                                                <%}%>

                                                    <% if(order.status==="returned" ){%>
                                                        <h6>status: <span class="text-primary">Order <%=order.status%>
                                                                    </span></h6>
                                                        <h6>Reason for Return : <span class="text-muted"
                                                                style="font-size: 12px;">
                                                                <%=order.returnedReason%>
                                                            </span></h6>
                                                        <%}%>
                                                            <%if(order.status==="payment failed" ){%>
                                                                <h6>status: <span class="text-danger">
                                                                        <%=order.status%>
                                                                    </span></h6>
                                                                <%}%>
                                                                    <%}%>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
