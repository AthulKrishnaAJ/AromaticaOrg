	<script>
		function removeProduct(productId, event) {
			event.preventDefault();
			let discountPrice = parseFloat($('#couponDiscount').val()) || 0

			Swal.fire({
				title: "Are You Sure..?",
				text: "You want to delete this item...!",
				icon: "warning",
				showCancelButton: true,
				confirmButtonColor: '#3085d6',
				cancelButtonColor: '#d33',
				confirmButtonText: 'Yes, remove it!'
			}).then((result) => {
				if (result.isConfirmed) {
					confirmRemoveProduct(productId, discountPrice);
				}
			})
		}

		function confirmRemoveProduct(productId, discountPrice) {
			$.ajax({
				url: "/removeProduct",
				method: "post",
				data: { productId, discountPrice },
				success: (response) => {
					if (response.status === true) {
						console.log("Product removed successfully");
						localStorage.setItem('couponDiscount', 0);
						Swal.fire({
							title: "Success!",
							text: "Product has been removed from the cart",
							icon: "success",
							timer: 2000,
							showConfirmButton: false,
						}).then(() => {
							location.reload();
						})
					}
					else {
						Swal.fire({
							title: "Error..!",
							text: response.message,
							icon: "error",
							showConfirmButton: true

						})
					}
				},
				error: (error) => {
					console.log("Ajaxxxxxxxx error : ", error);
					Swal.fire({
						title: "Somthing Wrong",
						text: "Failed to remove",
						icon: "error",
						showConfirmButton: true
					})
				}
			})
		}
	</script>




