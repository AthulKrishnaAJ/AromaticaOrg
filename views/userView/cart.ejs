	<script>



		$(document).ready(function(){
			$('#clearShopingCartBtn').click(function(){
				Swal.fire({
					title: 'Are you sure!',
					text: 'You want to clear this cart',
					icon: 'question',
					showCancelButton: true,
					confirmButtonText: 'Ok',
				}).then( async(result) => {
					if(result.isConfirmed){

						await fetch('/clearCart', {
							method : 'put',
						}).then(response => response.json())
						.then(data => {
							if(data.success){
								window.location.reload()
							}
							else{
								Swal.fire({
									title: 'Oops!',
									text: data.message,
									timer: 3000,
									icon: 'error',
									confirmButtonText: 'Ok'
								})
							}
						})
						.catch(error => {
							console.log('Error in clearing cart', error);
						})
					}
				})
			})
		})


		$(document).ready(function () {

			const discountFromStorage = localStorage.getItem('couponDiscount');
			if (discountFromStorage) {
				$('#couponDiscount').val(discountFromStorage);
			}

			$('#couponForm').submit(function (event) {
				event.preventDefault();

				let couponCode = $('#couponInput').val().trim();

				// validation
				if (couponCode === '' || couponCode === null) {
					$('#couponErrorMessage').text('Field is require');
					return false;
				}
				else {
					$('#couponErrorMessage').text('');

				}

				let cartEmptyMessage = $('#cartEmptyMessage').text();
				if (cartEmptyMessage === 'Cart is empty') {
					Toastify({
						text: 'Cart is empty cannot apply coupon',
						duration: 2000,
						close: true,
						gravity: 'top',
						position: 'right',
						backgroundColor: '#dc3545'
					}).showToast();
					return false;
				}

				$.ajax({
					url: '/applyCoupon',
					method: 'post',
					data: { couponCode: couponCode },
					success: (response) => {
						if (response.success) {
							const discount = response.coupon.discount
							const couponApplied = response.userCart.couponApplied

							couponAppliedGrandTotal(discount);
							localStorage.setItem('couponDiscount', discount)

							Toastify({
								text: response.message,
								duration: 2000,
								gravity: 'top',
								position: 'right',
								backgroundColor: '#28a745'
							}).showToast();

						}
						else {

							Toastify({
								text: response.message,
								duration: 2000,
								gravity: 'top',
								position: 'right',
								backgroundColor: '#dc3545'
							}).showToast();
						}

					},
					error: (error) => {
						console.error("Error in ajaxxx :", error);
					}
				})
			})



			$('#removeCouponButton').click(function (event) {
				event.preventDefault();

				let couponCode = $('#couponInput').val().trim();

				// validation
				if (couponCode === '' || couponCode === null) {
					$('#couponErrorMessage').text('Field is require');
					return false;
				}
				else {
					$('#couponErrorMessage').text('');

				}
				$.ajax({
					url: '/removeCoupon',
					method: 'post',
					success: (response) => {
						if (response.success) {

							const totalCost = parseFloat(response.userCart.totalCost);
							$('#grandTotal').text(`Rs.${totalCost}`);

							Toastify({
								text: response.message,
								duration: 2000,
								gravity: 'top',
								position: 'right',
								backgroundColor: '#dc3545'
							}).showToast();

						}
						else {

							Toastify({
								text: response.message,
								duration: 2000,
								gravity: 'top',
								position: 'right',
								backgroundColor: '#dc3545'
							}).showToast();
						}


					}
				})
			})
		})

		function couponAppliedGrandTotal(discount) {

			let grandTotal = parseFloat($('#grandTotal').text().replace('Rs.', ''));
			const newGrandTotal = grandTotal - parseFloat(discount);
			$('#grandTotal').text(`Rs.${newGrandTotal}`);
			$('#couponDiscount').val(discount);

		}





