    <script>
        $('#emailVerificationForm').submit(function (event) {
            event.preventDefault();

            let email = $('#emailInput').val();

            $.ajax({
                url: '/emailVerification',
                method: 'post',
                data: { email: email },
                success: (response) => {
                    if (response.success) {

                        Toastify({
                            text: response.message,
                            duration: 3000,
                            close: true,
                            style: {
                                fontSize: "12px",
                            }
                        }).showToast();


                    }
                    else {

                        let errorMessage = document.getElementById('alertMessage');
                        errorMessage.textContent = response.message

                        setTimeout(() => {
                            errorMessage.style.display = "none"
                        }, 4000)


                    }
                },
                error: (error) => {
                    console.log("ajaxx error...", error.message);

                }
            })

        })


    </script>









