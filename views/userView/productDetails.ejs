    <script>

        $(document).ready(function () {
            $('.addToWishlist').on('click', function () {
                let productId = $(this).data('product-id')

                $.ajax({
                    url: '/wishlist',
                    method: 'post',
                    data: { productId },
                    success: (response) => {
                        if (response.status === true) {
                            Swal.fire({
                                title: 'Success!',
                                text: response.message,
                                icon: 'success',
                                timer: 3000
                            })
                        }
                        else {
                            Swal.fire({
                                title: 'Warning!',
                                text: response.message || "Please login",
                                icon: 'warning',
                                timer: 3000
                            })
                        }
                    },
                    error: (error) => {
                        console.log("ajaxxx error : ", error)
                        Swal.fire({
                            title: 'Error',
                            text: 'somthing went wrong please try again',
                            icon: 'error',
                            timer: 3000
                        })
                    }
                })
            })
        })








        function addToCart(productId) {
            let quantity = 1
            $.ajax({
                url: `/addToCart?id=${productId}&quantity=${quantity}`,
                method: "post",
                success: (res) => {
                    if (res.status === true) {
                        console.log("Enter the success response block");
                        Swal.fire({
                            title: "Added to Cart !",
                            text: "Continue Shopping",
                            icon: "success",
                            timer: 2000,
                            showConfirmButton: false
                        });
                    }
                    else {
                        if (res.status === false && res.message === "Out of stock") {

                            Swal.fire({
                                title: "Out of stock",
                                text: "Failed to add cart",
                                icon: "warning",
                                showConfirmButton: true
                            });
                        }
                        else if (res.status === "Product Unavailable") {

                            Swal.fire({
                                title: "Product Unavailable",
                                text: "Selected product is unavailble",
                                icon: "warning",
                                showConfirmButton: true
                            });
                        }
                        else {

                            Swal.fire({
                                title: "Please Login",
                                text: "Failed to add to cart",
                                icon: "warning",
                                showConfirmButton: true
                            }).then(() => {
                                window.location.href = "/login"
                            })
                        }



                    }

                },

                error: (error) => {
                    console.log("Ajax error", error.message);

                    Swal.fire({
                        title: "Something wrong!",
                        text: "Failed to add to cart",
                        icon: "error",
                        showConfirmButton: true
                    });

                }
            });
        }

    </script>



