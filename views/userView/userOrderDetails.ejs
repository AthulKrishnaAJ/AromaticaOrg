    <section class="content-main container">
        <div class="content-header">
            <div>
                <hr>
                <h2 class="content-title card-title">Order detail</h2>
            </div>
        </div>

        <div class="card">
            <header class="card-header">
                <div class="row align-items-center">
                    <div class="col-lg-6 col-md-6 mb-lg-0 mb-15">
                        <!-- <span>
                        <i class="material-icons md-calendar_today"></i> <b>Wed, Aug 13, 2020, 4:34PM</b>
                    </span> <br>
                    <small class="text-muted" style="font-size: medium; font-weight: 400;">Order ID: </small> -->
                    </div>

                </div>
            </header> <!-- card-header end// -->
            <br>
            <div class="card-body">
                <div class="row mb-50 mt-20 order-info-wrap bg-light">
                    <div class="col-md-4 text-center">
                        <article class="icontext align-items-start">
                            <span class="icon icon-sm rounded-circle bg-primary-light">
                                <i class="text-primary material-icons md-person"></i>
                            </span>
                            <div class="text">
                                <h6 class="mb-1">Customer</h6>
                                <p class="mb-1">
                                    Name :<b>
                                        <%=order.user.name%>
                                    </b>
                                    <template class="__cf_email__"
                                        data-cfemail="e2838e879aa2879a838f928e87cc818d8f">[email&#160;protected]</template>
                                    <br>Mobile :<b>
                                        <%=order.user.mobile%>
                                    </b>
                                </p>
                                <!-- <a href="#">View profile</a> -->
                            </div>
                        </article>
                    </div> <!-- col// -->
                    <div class="col-md-4 text-center">
                        <article class="icontext align-items-start">
                            <span class="icon icon-sm rounded-circle bg-primary-light">
                                <i class="text-primary material-icons md-local_shipping"></i>
                            </span>
                            <div class="text">
                                <h6 class="mb-1">Order info</h6>
                                <p class="mb-1">
                                    Order Id : <b>
                                        <%=order._id%>
                                    </b> <br> Pay method :<b>
                                        <%=order.paymentMethod%>
                                    </b> <br> Status:<b>
                                        <%=order.status%>
                                    </b>
                                </p>
                                <!-- <a href="#">Download info</a> -->
                            </div>
                        </article>
                    </div> <!-- col// -->
                    <div class="col-md-4 text-center">
                        <article class="icontext align-items-start">
                            <span class="icon icon-sm rounded-circle bg-primary-light">
                                <i class="text-primary material-icons md-place"></i>
                            </span>
                            <div class="text">
                                <h6 class="mb-1">Deliver to</h6>
                                <p class="mb-1">
                                    City: <b>
                                        <%=order.address.city%>, <%=order.address.state%>
                                    </b>
                                </p>
                                <p class="mb-1">
                                    Address : <b>
                                        <%=order.address.addressLine%>, <%=order.address.pincode%>,
                                    </b>
                                    <input type="hidden" id="orderId" name="orderId"
                                        value="<%= order.address.addressId%>">
                                    <input type="hidden" name="paymentMethod" id="paymentMethod"
                                        value="<%= order.paymentMethod%>">
                                </p>
                                <!-- <a href="#">View profile</a> -->
                            </div>
                        </article>
                    </div> <!-- col// -->
                </div> <!-- row // -->
                <br>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="table-responsive">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th width="40%" class="fw-bold">Product</th>

                                        <th width="20%" class="fw-bold text-center">Unit Price</th>
                                        <th width="20%" class="fw-bold text-center">Quantity</th>
                                        <th width="20%" class="fw-bold text-end text-center">Total</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% order.items.forEach((item,index)=> {%>
                                        <tr>
                                            <td>
                                                <a class="itemside" href="#">
                                                    <div class="left">
                                                        <img src="/uploads/product-images/<%=item.productId.productImage[0]%>"
                                                            width="40" height="40" class="img-xs" alt="Product image">
                                                    </div>
                                                    <div class="info" style="font-size: 12px;">
                                                        <%=item.productId.productName%>
                                                    </div>
                                                </a>
                                            </td>

                                            <td class="text-center" style="vertical-align: middle; font-size: 12px;">
                                                Rs.<%=item.productId.salePrice%>.00</td>

                                            <td class="text-center" style="vertical-align: middle; font-size: 12px;">
                                                <%=item.quantity%>
                                            </td>
                                            <td class="text-end text-center"
                                                style="vertical-align: middle; font-size: 12px;"> Rs.<%=item.price%>.00
                                            </td>
                                        </tr>
                                        <%})%>
                                            <tr>
                                                <td colspan="4">
                                                    <article class="float-end">
                                                        <div class="order-summary">
                                                            <div class="order-summary-row">
                                                                <div class="order-summary-label fs-5">Grand total : Rs.
                                                                    <%=order.totalCost%>.00
                                                                </div>
                                                                <div class="order-summary-value fs-5"></div>
                                                            </div>
                                                            <div class="order-summary-row">
                                                                <div class="order-summary-label">Status :
                                                                    <% if(order.status==="pending" ){%>
                                                                        <span
                                                                            class="badge rounded-pill alert-warning text-warning"
                                                                            style="font-size: 12px;">
                                                                            <%=order.status%>
                                                                        </span>
                                                                        <%} else if(order.status==="processing" ){%>
                                                                            <span
                                                                                class="badge rounded-pill alert-secondary text-secondary"
                                                                                style="font-size: 12px;">
                                                                                <%=order.status%>
                                                                            </span>
                                                                            <%} else if(order.status==="shipped" ){%>
                                                                                <span
                                                                                    class="badge rounded-pill alert-info text-info"
                                                                                    style="font-size: 12px;">
                                                                                    <%=order.status%>
                                                                                </span>
                                                                                <%} else if(order.status==="delivered"
                                                                                    ){%>
                                                                                    <span
                                                                                        class="badge rounded-pill alert-success text-success"
                                                                                        style="font-size: 12px;">
                                                                                        <%=order.status%>
                                                                                    </span>
                                                                                    <%} else
                                                                                        if(order.status==="cancelled"
                                                                                        ){%>
                                                                                        <span
                                                                                            class="badge rounded-pill alert-danger text-danger"
                                                                                            style="font-size: 12px;">
                                                                                            <%=order.status%>
                                                                                        </span>
                                                                                        <%}else
                                                                                            if(order.status==="returned"
                                                                                            ){%>
                                                                                            <span
                                                                                                class="badge rounded-pill alert-primary text-primary"
                                                                                                style="font-size: 12px;">
                                                                                                <%=order.status%>
                                                                                            </span>
                                                                                            <%} else
                                                                                                if(order.status==="return pending"
                                                                                                ){%>
                                                                                                <span
                                                                                                    class="badge rounded-pill alert-danger text-danger"
                                                                                                    style="font-size: 12px;">
                                                                                                    <%=order.status%>
                                                                                                </span>
                                                                                                <%}else
                                                                                                    if(order.status==="payment failed"
                                                                                                    ){%>
                                                                                                    <span
                                                                                                        class="badge rounded-pill alert-danger text-danger"
                                                                                                        style="font-size: 12px;">
                                                                                                        <%=order.status%>
                                                                                                    </span>
                                                                                                    <%}%>
                                                                </div>

                                                            </div>
                                                        </div>
                                                        <% if(order.status !=="cancelled" && order.status !=="delivered"
                                                            && order.status !=="returned" && order.status
                                                            !=="return pending" && order.status !=="payment failed" ){%>
                                                            <button class="btn btn-danger return-order-button"
                                                                onclick="" id="cancelOrderBtn">
                                                                <span>Cancel Order</span>
                                                            </button>
                                                            <%}%>
                                                                <!-- <% const expiryOptions = { day: '2-digit', month: '2-digit', year: 'numeric' }; %> -->
                                                                <% if(order.status==="delivered" ){%>
                                                                    <button class="btn btn-primary return-order-button"
                                                                        id="returnOrderBtn" onclick="">
                                                                        <span>Return Order</span>
                                                                    </button>
                                                                    <%}%>

                                                                        <%if(order.status==="payment failed" ){%>
                                                                            <button
                                                                                class="btn btn-success return-order-button"
                                                                                id="payAgainButton"
                                                                                data-paymentMethod="razorpay">
                                                                                <span>Pay again</span>
                                                                            </button>
                                                                            <%}%>
                                                                                <!-- <div class="expiry-date">Expiry Date: </div>
                                            <div class="expiry-date">Expiry Date: Expired</div>
                                            <div class="cannot-return">Cannot be returned</div> -->
                                                    </article>
                                                </td>

                                            </tr>
                                            <tr>
                                                <% if(order && order.status==='delivered' ){%>
                                                    <td colspan="4">
                                                        <input type="hidden" value="<%= JSON.stringify(order)%>"
                                                            id="orderData">
                                                        <button class="btn btn-secondary float-start ml-5"
                                                            id="invoiceButton" onclick="invoiceDownload()">Download
                                                            Invoice</button>
                                                    </td>
                                                    <%}%>
                                            </tr>
                                </tbody>
                            </table>
                            <input type="hidden" id="orderDetails" value="">
                            <input type="hidden" name="" id="productDetails" value="">
                        </div> <!-- table-responsive// -->
                    </div> <!-- col// -->
                    <div class="col-lg-1"></div>
                </div>
            </div> <!-- card-body end// -->
        </div> <!-- card end// -->
    </section> <!-- content-main end// -->



