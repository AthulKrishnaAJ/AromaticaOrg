        function toggleLoadingModal() {
            $('#loadingModal').modal('show');
        }

        // pay again razorpay
        $(document).ready(function () {
            $('#payAgainButton').click(function (event) {
                event.preventDefault();

                const selectedAddressId = $('#orderId').val();
                const paymentMethod = $('#paymentMethod').val();


                $.ajax({
                    url: '/initiateRazorpay',
                    method: 'post',
                    success: (response) => {
                        if (response.success) {

                            openRazorpayModal(response.razorpayOrder, response.keyId, response.user, selectedAddressId, paymentMethod);
                        }
                    },
                    error: (error) => {
                        console.log("Error in ajax :", error);
                        Swal.fire({
                            title: 'Oops',
                            text: 'Failed to placed your order, please try agian',
                            icon: 'error'
                        });
                    }
                })
            })
        });



        function openRazorpayModal(razorpayOrder, keyId, user, selectedAddressId, paymentMethod) {

            const options = {
                key: keyId,
                amount: razorpayOrder.amount,
                currency: razorpayOrder.currency,
                name: 'Aromatica',
                order_id: razorpayOrder._id,
                handler: function () {

                    placeOrder(selectedAddressId, paymentMethod);

                },
                prefill: {
                    name: user.name,
                    email: user.email,
                    contact: user.mobile
                },
                theme: {
                    color: '#F37254',

                }
            };
            let rzp = new Razorpay(options);

            rzp.on('payment.failed', function (response) {
                handleRazorapyFailure(selectedAddressId, paymentMethod)

            });
            rzp.open();

        }



        function placeOrder(selectedAddressId, paymentMethod) {

            $.ajax({
                url: "/placeOrder",
                method: "post",
                data: {
                    selectAddressId: selectedAddressId,
                    paymentMethod: paymentMethod
                },
                success: (response) => {
                    if (response.success) {
                        toggleLoadingModal();
                        setTimeout(() => {

                            window.location.href = "/orderSuccess?orderId=" + response.orderId

                        }, 4000);
                    }
                    else {
                        Swal.fire({
                            icon: 'error',
                            title: 'Oops...',
                            text: 'Failed to initiate payment, please try again.'
                        });
                    }
                },
                error: (error) => {
                    Swal.fire({
                        icon: 'error',
                        title: 'Oops...',
                        text: 'Something went wrong, please try again'
                    })
                }
            })
        }



        function handleRazorapyFailure(selectedAddressId, paymentMethod) {
            //  alert('enter the failure function')
            $.ajax({
                url: '/savaRazorpayFailure',
                method: 'post',
                data: {
                    selectedAddressId: selectedAddressId,
                    paymentMethod: paymentMethod
                },
                success: (response) => {
                    if (response.success) {
                        Toastify({
                            text: 'Payment failed',
                            duration: 3000,
                            close: true,
                            gravity: 'top',
                            position: 'left',
                            backgroundColor: '#dc3545',
                        }).showToast();
                    }
                    else {
                        Swal.fire({
                            icon: 'error',
                            title: 'Oops...',
                            text: 'Payment failure failed, please contact support.',

                        });
                    }
                },
                error: (error) => {
                    console.log("Error in ajax :", error);
                    Swal.fire({
                        icon: 'error',
                        title: 'Oops...',
                        text: 'Somthing went wrong, please try again',

                    });
                }
            })
        }





        // return order
        $(document).ready(function () {
            $('#returnOrderBtn').click(function () {

                $('#returnOrderModal').modal('show')

                $('#returnOrderForm').submit(function (event) {
                    event.preventDefault();

                    const returnReason = $('#returnReason').val()

                    if (returnReason.trim() === '' || returnReason.trim() === null) {
                        $('#returnReasonError').text('Field is required');
                        return false;
                    }
                    else if (returnReason.trim().length <= 9) {
                        $('#returnReasonError').text('Enter atleast 10 characters');
                        return false;
                    }


                    if (returnReason) {
                        $('#returnOrderModal').modal('hide');
                    }


                    $.ajax({
                        url: `/returnOrder/<%=order._id%>`,
                        method: 'post',
                        contentType: 'application/json',
                        data: JSON.stringify({ reason: returnReason }),
                        success: (response) => {
                            if (response.success) {
                                Toastify({
                                    text: response.message,
                                    duration: 3000,
                                    close: true,
                                    backgroundColor: "#28a745",
                                    style: {
                                        fontSize: "12px",

                                    }
                                }).showToast();
                                setTimeout(() => {
                                    location.reload()
                                }, 3000)

                            }
                            else {
                                Swal.fire('Warning!', response.message, 'warning')
                            }

                        },
                        error: (error) => {
                            console.log(`ajaxxx error => ${error}`)
                            Swal.fire({
                                title: "Error!",
                                text: "Somthing went wrong please try again",
                                icon: 'error',
                                timer: 3000
                            })
                        }
                    })
                })

            })
        })





        // cancel order
        $(document).ready(function () {
            $('#cancelOrderBtn').click(function () {

                $('#cancelOrderModal').modal('show')

                $('#cancelOrderForm').submit(function (event) {
                    event.preventDefault()

                    const cancellationReason = $('#cancellationReason').val()

                    if (cancellationReason.trim() === '' || cancellationReason.trim() === null) {
                        $('#reasonError').text('Field is required');
                        return false
                    }
                    else if (cancellationReason.trim().length <= 9) {
                        $('#reasonError').text('Enter atleast 10 characters');
                        return false
                    }

                    if (cancellationReason) {
                        $('#cancelOrderModal').modal('hide')
                    }

                    $.ajax({
                        url: '/cancelOrderByUser/<%=order._id%>',
                        method: 'post',
                        contentType: 'application/json',
                        data: JSON.stringify({ reason: cancellationReason }),
                        success: (response) => {
                            if (response.status === true) {

                                Swal.fire("Cancelled!", response.message, 'success')
                                    .then(() => {
                                        location.reload()
                                    });
                            }
                            else {
                                Swal.fire("Warning!", response.message, 'warning');
                            }
                        },
                        error: (error) => {
                            console.log("ajaxxx error", error);
                            Swal.fire({
                                title: "Error!",
                                text: "Somthing went wrong please try again",
                                icon: 'error',
                                timer: 3000
                            })
                        }
                    })


                })
            });
        });
    </script>




